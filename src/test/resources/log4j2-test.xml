<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d %-5p %C (%F:%L) - %m%n"/>
		</Console>
	</appenders>
	<loggers>
		<root level="debug">
			<appender-ref ref="STDOUT"/>
		</root>
	</loggers>
</configuration>